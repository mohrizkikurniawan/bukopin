    SUBROUTINE ATI.VID.BEN.CHECK.ID
*-----------------------------------------------------------------------------
* Developer Name     : Dhio Faizar Wahyudi
* Development Date   : 20191206
* Description        : Routine for check PAYEE exist or no
*-----------------------------------------------------------------------------
* Modification History:-
*-----------------------------------------------------------------------------
* Date            Modified by                Description
*-----------------------------------------------------------------------------
*
*-----------------------------------------------------------------------------
    $INSERT I_COMMON
    $INSERT I_EQUATE
    $INSERT I_F.BENEFICIARY
	
*-----------------------------------------------------------------------------
MAIN:
*-----------------------------------------------------------------------------
    GOSUB INIT
	GOSUB PROCESS
	
    RETURN

*-----------------------------------------------------------------------------
INIT:
*-----------------------------------------------------------------------------
	
	FN.BENEFICIARY = 'F.BENEFICIARY'
	F.BENEFICIARY  = ''
	CALL OPF(FN.BENEFICIARY, F.BENEFICIARY)
	
    RETURN

*-----------------------------------------------------------------------------
PROCESS:
*-----------------------------------------------------------------------------
	
	CALL F.READ(FN.BENEFICIARY, COMI, R.BENEFICIARY, F.BENEFICIARY, BENEFICIARY.ERR)
	IF R.BENEFICIARY EQ '' THEN
		E = 'EB-OK.OCE.PAYEE.MISSING'
	END
	
    RETURN
*-----------------------------------------------------------------------------
END




